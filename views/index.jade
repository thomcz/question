include layout.jade
    header
      h3.header-h3
        i.w3-opennav.fa.fa-bars(onclick='w3_open()')
        |   Questionater
    nav.w3-sidenav.w3-card-2.w3-white.w3-top(style='width:70%;display:none')
      .w3-container.w3-theme-d2
        span.w3-closenav.w3-right.w3-xlarge(onclick='w3_close()') x
        br
        .w3-padding.w3-center
          p
            i.fa.fa-question-circle(style='font-size:100px;')
      br
      a(href='#') Movies
      a(href='#') Friends
      a(href='#') Messages
    .w3-container.content
      hr
      each val in questions
        .w3-row(onclick='openMyQuestion(#{val})')
          .w3-col.s9.w3-container(style="margin-top:-20px; margin-bottom:-40px")
            h4.w3-text-red.ellipsis
              | #{val.text}
            p.ellipsis 3 bannanen ssssssssssssssssssssssssssssssssssssssund eine birne
          .w3-col.s1.w3-container(style='margin-top:-20px;')
            i.fa.fa-thumbs-o-up
          .w3-col.s1.w3-container
            p #{val.votes}
        hr
    include footer.jade
